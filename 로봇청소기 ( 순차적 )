        "# prompt: \n",
        "\n",
        "import turtle\n",
        "\n",
        "#  \n",
        "screen = turtle.Screen()\n",
        "screen.setup(width=600, height=600)\n",
        "screen.tracer(0) #   \n",
        "\n",
        "#   () \n",
        "robot = turtle.Turtle()\n",
        "robot.speed(0) #    ( )\n",
        "robot.color(\"blue\") #  \n",
        "robot.shape(\"square\") #  \n",
        "robot.penup() #   (      )\n",
        "robot.goto(-100, -100) #   \n",
        "robot.pendown() #   (  )\n",
        "robot.pensize(3) #   \n",
        "\n",
        "#      \n",
        "side_length = 200\n",
        "speed = 10 #   (  )\n",
        "\n",
        "#  \n",
        "for _ in range(4):\n",
        "    #  \n",
        "    for _ in range(side_length // speed): #       \n",
        "        robot.forward(speed)\n",
        "        screen.update() #   ( )\n",
        "\n",
        "    # 90 \n",
        "    robot.right(90)\n",
        "\n",
        "#    \n",
        "screen.mainloop()\n"
      ]
